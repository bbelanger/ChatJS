@using ChatJsMvcSample.Models
@{
    this.Layout = "../_Layout.cshtml";
    var userInfo = (UserViewModel)this.ViewBag.CurrentUser;
}

<div class="container">
    <div class="jumbotron">
        <img src="@userInfo.ProfilePictureLargeUrl)" />
        <h4>You are logged in as @userInfo.EMail</h4>
        <p><a href="#">Signout</a></p>
    </div>
</div>
